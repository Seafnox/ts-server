{
  "name": "ts-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run tsc",
    "coverage": "npm run test-cov && npm run test-remap",
    "tslint": "tslint -p tsconfig.json",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc -w",
    "start": "nodemon --ignore 'node_modules/**/*' --exec ts-node server.ts",
    "run": "ts-node server.ts"
  },
  "dependencies": {
    "@tsed/common": "^5.1.1",
    "@tsed/core": "^5.1.1",
    "@tsed/di": "^5.1.1",
    "@tsed/multipartfiles": "^5.1.1",
    "@tsed/swagger": "^5.1.1",
    "@tsed/typeorm": "^5.1.1",
    "@types/swagger-schema-official": "^2.0.14",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "faker": "^4.1.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.3.4",
    "sql.js": "^0.5.0",
    "ts-httpexceptions": "^3.1.1",
    "typeorm": "^0.2.25",
    "unused-filename": "^1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.12.18",
    "@types/unused-filename": "^1.0.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.4"
  }
}
