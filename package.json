{
  "name": "ts-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run tsc",
    "coverage": "npm run test-cov && npm run test-remap",
    "tslint": "tslint -p tsconfig.json",
    "tslint:fix": "tslint -p tsconfig.json --fix",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc -w",
    "start": "nodemon --ignore 'node_modules/**/*' --exec ts-node bootstrap.ts",
    "run": "ts-node bootstrap.ts"
  },
  "dependencies": {
    "@tsed/common": "^6.75.1",
    "@tsed/core": "^6.75.1",
    "@tsed/di": "^6.75.1",
    "@tsed/platform-express": "^6.75.1",
    "@tsed/swagger": "^6.75.1",
    "@tsed/typeorm": "^6.75.1",
    "@types/swagger-schema-official": "^2.0.22",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.1",
    "sql.js": "^1.6.2",
    "ts-httpexceptions": "^4.1.0",
    "typeorm": "^0.2.38",
    "unused-filename": "^3.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/unused-filename": "^1.0.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.13",
    "ts-node": "^10.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.10"
  }
}
